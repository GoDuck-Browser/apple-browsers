  <file original="DuckDuckGo/Localizable.xcstrings" source-language="en" target-language="it_IT" datatype="plaintext">
    <header>
      <tool tool-id="com.apple.dt.xcode" tool-name="Xcode" tool-version="16.2" build-num="16C5032a" />
    </header>
    <body>
      <trans-unit id="pm.card.cardholder-name" xml:space="preserve">
        <source>Cardholder Name</source>
        <target>Nome del titolare</target>
        <note>Label for cardholder name title</note>
      </trans-unit>
      <trans-unit id="menu.add.folder" xml:space="preserve">
        <source>Add Folder…</source>
        <target>Aggiungi cartella…</target>
        <note>Menu item to add a folder</note>
      </trans-unit>
      <trans-unit id="close.other.tabs" xml:space="preserve">
        <source>Close Other Tabs</source>
        <target>Chiudi altre schede</target>
        <note>Menu item</note>
      </trans-unit>
      <trans-unit id="vpn.no.exclusions.found.text" xml:space="preserve">
        <source>None</source>
        <target>Nessuna</target>
        <note>Text shown in VPN settings when no exclusions are configured</note>
      </trans-unit>
      <trans-unit id="pm.deactivate" xml:space="preserve">
        <source>Deactivate</source>
        <target>Disattiva</target>
        <note>Deactivate button</note>
      </trans-unit>
      <trans-unit id="pinned.tabs.mode.separate" xml:space="preserve">
        <source>Separate in each window</source>
        <target>Separate in ogni finestra</target>
        <note>Preferences &gt; Tabs &gt; Pinned tabs are different in each window</note>
      </trans-unit>
      <trans-unit id="preferences.off" xml:space="preserve">
        <source>Off</source>
        <target>Disattivato</target>
        <note>Status indicator of a browser privacy protection feature.</note>
      </trans-unit>
      <trans-unit id="preferences-tabs.title" xml:space="preserve">
        <source>Tabs</source>
        <target>Schede</target>
        <note>Title for tabs section in settings</note>
      </trans-unit>
      <trans-unit id="pm.note" xml:space="preserve">
        <source>Note</source>
        <target>Nota</target>
        <note>Label for note title</note>
      </trans-unit>
      <trans-unit id="pinning.show-netp-shortcut" xml:space="preserve">
        <source>Show VPN Shortcut</source>
        <target>Mostra scorciatoia VPN</target>
        <note>Menu item for showing the NetP shortcut</note>
      </trans-unit>
      <trans-unit id="import.csv.instructions.opera" xml:space="preserve">
        <source>%1$d Open **%2$s**
%3$d Use the Menu Bar to select **View → Show Password Manager**
%4$d Select **Settings**
%5$d Find "Export Passwords" and click **Download File**
%6$d Save the passwords file someplace you can find it (e.g., Desktop)
%7$d %8$@</source>
        <target>%1$d Apri **%2$s**
%3$d Usa la barra dei menu per selezionare **Visualizza → Mostra Password Manager**
%4$d Seleziona **Impostazioni**
%5$d Trova "Esporta password" e clicca su **Scarica file**
%6$d Salva il file delle password in un posto dove puoi trovarlo (es. Desktop)
%7$d %8$@</target>
        <note>Instructions to import Passwords as CSV from Opera browser.
%N$d - step number
%2$s - browser name (Opera)
%8$@ - "Select Passwords CSV File" button
**bold text**; _italic text_</note>
      </trans-unit>
      <trans-unit id="main-menu.file.close-all-windows" xml:space="preserve">
        <source>Close All Windows</source>
        <target>Chiudi tutte le finestre</target>
        <note>Main Menu File item</note>
      </trans-unit>
      <trans-unit id="history.view.onboarding.title" xml:space="preserve">
        <source>Easier History Management</source>
        <target>Gestione cronologia più semplice</target>
        <note>Title for the history view onboarding popover</note>
      </trans-unit>
      <trans-unit id="share.menu.item.qr.code" xml:space="preserve">
        <source>Create QR Code</source>
        <target>Crea codice QR</target>
        <note>Menu item title</note>
      </trans-unit>
      <trans-unit id="pm.sort.parameter.date-modified" xml:space="preserve">
        <source>Date Modified</source>
        <target>Data di modifica</target>
        <note>Label for Date Modified sort parameter</note>
      </trans-unit>
      <trans-unit id="pm.empty.cards.title" xml:space="preserve">
        <source>No Cards</source>
        <target>Nessuna carta</target>
        <note>Label for cards empty state title</note>
      </trans-unit>
      <trans-unit id="bookmarks.dialog.field.url" xml:space="preserve">
        <source>URL</source>
        <target>URL</target>
        <note>URL field label for Bookmar</note>
      </trans-unit>
      <trans-unit id="newTab.setup.import.summary" xml:space="preserve">
        <source>Import bookmarks, favorites, and passwords from your old browser.</source>
        <target>Importa segnalibri, preferiti e password dal tuo vecchio browser.</target>
        <note>Summary of the Import card of the Set Up section in the home page</note>
      </trans-unit>
      <trans-unit id="vpn.setting.add.excluded.domain.action.button.title" xml:space="preserve">
        <source>Exclude Website</source>
        <target>Escludi sito web</target>
        <note>Add excluded domain button title</note>
      </trans-unit>
      <trans-unit id="export.logins.failed.informative" xml:space="preserve">
        <source>Please check that no file exists at the location you selected.</source>
        <target>Verifica che non esistano file nella posizione selezionata.</target>
        <note>Alert message when exporting login data fails</note>
      </trans-unit>
      </body>
