  <file original="DuckDuckGo/Localizable.xcstrings" source-language="en" target-language="it_IT" datatype="plaintext">
    <header>
      <tool tool-id="com.apple.dt.xcode" tool-name="Xcode" tool-version="16.2" build-num="16C5032a" />
    </header>
    <body>
      <trans-unit id="pm.lock-screen.prompt.export-logins" xml:space="preserve">
        <source>export your usernames and passwords</source>
        <target>esportare i tuoi nomi utente e password</target>
        <note>Label presented when exporting logins</note>
      </trans-unit>
      <trans-unit id="dashboard.permission.ask" xml:space="preserve">
        <source>Ask every time</source>
        <target>Chiedi ogni volta</target>
        <note>Privacy Dashboard: Website should always Ask for permission for input media device access</note>
      </trans-unit>
      <trans-unit id="Left" xml:space="preserve">
        <source>Left</source>
        <target>Sinistra</target>
        <note>Preference title aligning the bookmarks bar</note>
      </trans-unit>
      <trans-unit id="tab.bookmarks.title" xml:space="preserve">
        <source>Bookmarks</source>
        <target>Segnalibri</target>
        <note>Tab bookmarks title</note>
      </trans-unit>
      <trans-unit id="tooltip.navigation.refresh" xml:space="preserve">
        <source>Reload this page</source>
        <target>Ricarica questa pagina</target>
        <note>Tooltip for the Refresh button</note>
      </trans-unit>
      <trans-unit id="preferences.autofill" xml:space="preserve">
        <source>Passwords</source>
        <target>Password</target>
        <note>Show Autofill preferences</note>
      </trans-unit>
      <trans-unit id="sync.promo.confirm.action" xml:space="preserve">
        <source>Set Up Sync</source>
        <target>Configura Sync</target>
        <note>Title for a button in the Sync Promotion banner to set up Sync</note>
      </trans-unit>
      <trans-unit id="import.browser.data" xml:space="preserve">
        <source>Import to DuckDuckGo</source>
        <target>Importa in DuckDuckGo</target>
        <note>Import Browser Data dialog title</note>
      </trans-unit>
      <trans-unit id="preferences.on-startup" xml:space="preserve">
        <source>On Startup</source>
        <target>All'avvio</target>
        <note>Name of the preferences section related to app startup</note>
      </trans-unit>
      <trans-unit id="page.error.header" xml:space="preserve">
        <source>DuckDuckGo can't load this page.</source>
        <target>DuckDuckGo non riesce a caricare questa pagina.</target>
        <note>Error page heading text</note>
      </trans-unit>
      <trans-unit id="duck-player.video-autoplay-preference" xml:space="preserve">
        <source>Autoplay videos when opened in Duck Player</source>
        <target>Riproduci automaticamente i video quando aperti in Duck Player</target>
        <note>Autoplay preference in settings</note>
      </trans-unit>
      <trans-unit id="open.settings" xml:space="preserve">
        <source>Open System Settings…</source>
        <target>Apri Impostazioni di Sistema…</target>
        <note>This string represents a prompt or button label prompting the user to open system settings</note>
      </trans-unit>
      <trans-unit id="autofill.onboarding.popover.title" xml:space="preserve">
        <source>Add passwords shortcut?</source>
        <target>Aggiungere scorciatoia per le password?</target>
        <note>Autofill onboarding popover title</note>
      </trans-unit>
      <trans-unit id="copy.image.address" xml:space="preserve">
        <source>Copy Image Address</source>
        <target>Copia indirizzo immagine</target>
        <note>Context menu item</note>
      </trans-unit>
      <trans-unit id="pm.address.postal-code" xml:space="preserve">
        <source>Postal Code</source>
        <target>Codice Postale</target>
        <note>Label for postal code title</note>
      </trans-unit>
      <trans-unit id="prefrences.sync.limit-exceeded-title" xml:space="preserve">
        <source>Sync Paused</source>
        <target>Sync in pausa</target>
        <note>Title for sync limits exceeded warning</note>
      </trans-unit>
      <trans-unit id="DuckDuckGo needs your permission to read the %@ bookmarks file. Select the %@ folder to import bookmarks." xml:space="preserve">
        <source>DuckDuckGo needs your permission to read the %1$@ bookmarks file. Select the %2$@ folder to import bookmarks.</source>
        <target>DuckDuckGo ha bisogno della tua autorizzazione per leggere il file dei segnalibri di %1$@. Seleziona la cartella %2$@ per importare i segnalibri.</target>
        <note>Data import warning that DuckDuckGo browser requires file reading permissions for another browser name (%1$@), and instruction to select its (same browser name - %2$@) bookmarks folder.</note>
      </trans-unit>
      </body>
  </file>
